  #{ /*$TREEFUN ?*/ (($TR) && TREE_CODE($TR) == FUNCTION_DECL) }#
  ;; fill expansion
  #{ /* $TREEFUN! */
   $TFUNAME = DECL_NAME($TR);
   $TINITIALDCL = DECL_RESULT($TR); 
  }#
)

