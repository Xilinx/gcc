		    (let ( (cslot (compile_obj ourslot gcx)) )
		      (debug_msg cslot "compilobj_nrep_fieldacc returning cslot")
		      (return cslot))
		  (return (makecompute))))
	    (return (makecompute)))
	  )
      (return (makecompute))
      )))
