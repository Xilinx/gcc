	    ))))
	

;; citerator on tuple
(defciterator foreach_in_multiple 
  (tup)					;start formal
  eachtup				;state
  (comp :long ix)			;local formals
  (					;before expansion
   "long " eachtup "_ln = basilys_multiple_length((basilys_ptr_t)" tup ");\n"
   "int " eachtup "_ix = 0;\n"
   "for ("  eachtup "_ix = 0; " eachtup "_ix < " eachtup "_ln; " 
         eachtup "_ix ++) {\n"
	 comp " = basilys_multiple_nth((basilys_ptr_t)(" tup "), " eachtup "_ix);\n"
	 ix " = " eachtup "_ix;\n"
   )
  (					;after expansion
   "} /* end " eachtup "*/"
   )
)

