    (write-string " \"" str)
    (map nil 
	 (lambda (c) 
	   (case c
		 (#\Newline (write-string "\\n" str))
		 (#\Tab  (write-string "\\t" str))
		 (#\\ (write-string "\\\\" str))
		 (#\' (write-string "\\'" str))
		 (#\" (write-string "\\\"" str))
		 (otherwise (if (standard-char-p c)
				(write-char c str)
			      (format str "\\x~2,'0x" (char-code c))))
		 )
	   )
	 cstri)
    (write-string "\" " str)
    ))


