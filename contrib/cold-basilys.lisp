;;; QUOTE syntax (only for symbols or keywords)
(defcoldmacro quote (nam args env)
  (declare (ignore nam))
  (if (rest args) (error "quote with more than one arg ~S" args))
  (let ((qarg (first args)))
    (or (symbolp qarg) (keywords qarg)
	(error "quote a non-symbol ~S" qarg))
